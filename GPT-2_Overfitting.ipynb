    "def generate_poem(model, tokenizer, text, max_length=100, top_k=50, top_p=0.95):\n",
    "    input_ids = tokenizer.encode(text, return_tensors=\"pt\").to('cuda:0')\n",
    "    \n",
    "    output = model.generate(input_ids, max_length=max_length, num_return_sequences=1, do_sample=True, top_k=top_k, top_p=top_p, temperature=1)\n",
    "    return tokenizer.decode(output[0], skip_special_tokens=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "2ed041e0-b16e-4f1c-bcec-51841b505fbe",
   "metadata": {},
   "outputs": [],
   "source": [
    "def generate_poems(model, tokenizer, test_set, max_length=100, top_k=50, top_p=0.95):\n",
    "    p = {}\n",
    "    \n",
    "    for i in range(samples):\n",
    "        p[test_set['Content'][i]] = []\n",
    "        for params in generation_params:\n",
    "            for j in range(4):\n",
    "               p[test_set['Content'][i]].append(generate_poem(model, tokenizer, test_set['Content'][i][0:35], 100, **params))\n",
    "\n",
    "    return p"
   ]
  },
